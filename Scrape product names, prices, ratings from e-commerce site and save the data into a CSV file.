#include <stdio.h>
struct Product {
    char name[50];
    float price;
    float rating;
};
int main() {
    struct Product products[] = {
        {"Gaming Keyboard", 2299.00, 4.6},
        {"Noise Cancelling Headphones", 7499.99, 4.8},
        {"Smart LED Bulb", 699.00, 4.4},
        {"Portable SSD 1TB", 5999.00, 4.5}
    };
    int count = sizeof(products) / sizeof(products[0]);
    printf("Name,Price,Rating\n");
    for (int i = 0; i < count; i++) {
        printf("%s,%.2f,%.1f\n", products[i].name, products[i].price, products[i].rating);
    }
    printf("Product data printed in CSV format.\n");
    return 0;
}
